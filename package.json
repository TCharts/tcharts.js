{
  "name": "tcharts.js",
  "version": "0.0.3",
  "description": "Lightweight and fast terminal ASCII charts for nodejs and browser.",
  "main": "dist/tcharts.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint src && eslint __tests__",
    "cover": "jest --coverage",
    "test": "npm run lint && npm run cover",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "build": "webpack && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hustcc/tcharts.js.git"
  },
  "keywords": [
    "terminal",
    "console",
    "charts",
    "ascii",
    "terminal-charts",
    "tcharts",
    "tcharts.js"
  ],
  "author": "hustcc",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hustcc/tcharts.js/issues"
  },
  "homepage": "https://github.com/hustcc/tcharts.js#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "jest": "^20.0.4",
    "webpack": "^3.0.0"
  },
  "dependencies": {
    "area-divide": "^0.0.1",
    "evenly": "^1.0.2",
    "fixed-round": "^0.0.1",
    "immutability-util": "^0.0.4",
    "variable-type": "^0.1.1",
    "what.js": "^1.0.1",
    "word-width": "^1.0.1"
  },
  "jest": {
    "testRegex": "(/__tests__/index)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/*.{js,jsx}",
      "src/*/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    }
  }
}
